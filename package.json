{
  "name": "@quarter/simple-ui",
  "version": "0.0.1-alpha",
  "author": {
    "name": "Quarter",
    "email": "master@shiruihua.cn"
  },
  "description": "A Simple UI Kit",
  "keywords": [
    "vue2",
    "UI Kit"
  ],
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/unmian/-quarter-simple-ui"
  },
  "files": [
    "lib",
    "types"
  ],
  "main": "lib/index.umd.js",
  "module": "lib/index.es.js",
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js"
    },
    "./lib/style.css": {
      "import": "./lib/style.css",
      "require": "./lib/style.css"
    }
  },
  "typings": "types/index.d.ts",
  "homepage": "https://github.com/unmian/-quarter-simple-ui",
  "bugs": "https://github.com/unmian/-quarter-simple-ui/issues",
  "scripts": {
    "dev": "vite --config ./build/base.config.ts",
    "test": "vue-tsc --noEmit",
    "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
    "build:doc": "vue-tsc --noEmit && vite build --config ./build/doc.config.ts",
    "preview": "vite preview",
    "publish:private": "npm publish --registry https://npm.lescity.com.cn/",
    "unpublish:private": "npm unpublish --force --registry https://npm.lescity.com.cn/"
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/runtime-dom": "^3.2.31",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "sass": "^1.49.4",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-dts": "^0.9.9",
    "vite-plugin-vue2": "^1.9.3",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.31.4"
  },
  "dependencies": {
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.3",
    "vuex": "^3.6.2"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "ie >= 10",
    "iOS >= 8",
    "Android >= 4"
  ]
}
